            "jsonSpec": {
              "expr": {
                "Literal": {
                  "Value": "'{\n  \"$schema\": \"https://vega.github.io/schema/vega-lite/v5.json\",\n  \"description\": \"Plot showing a 30 day rolling average with raw values in the background.\",\n\n  \"data\": {\n    \"name\": \"dataset\" // data source reference\n  },\n\n  // TRANSFORM SECTION\n  \"transform\": [\n    {\n      // WINDOW TRANSFORM (creates a rolling mean over time)\n      \"window\": [\n        {\n          \"field\": \"temp_max\", // numeric field to average\n          \"op\": \"mean\", // operation: calculate mean\n          \"as\": \"rolling_mean\" // new field name for the computed average\n        }\n      ],\n      \"frame\": [-15, 15] // 30-day window centered on each row (15 days before & after)\n    }\n  ],\n\n  // GLOBAL ENCODING (shared by all layers)\n  \"encoding\": {\n    \"x\": {\n      \"field\": \"date\", // use date field for x-axis\n      \"type\": \"temporal\", // time-based axis\n      \"title\": \"Date\" // axis label\n    },\n    \"y\": {\n      \"type\": \"quantitative\", // numeric y-axis\n      \"axis\": {\n        \"title\": \"Max Temperature and Rolling Mean\" // shared y-axis title\n      }\n    }\n  },\n\n  // LAYERED VISUALIZATION\n  \"layer\": [\n    {\n      // LAYER 1: raw temperature values (background points)\n      \"mark\": {\n        \"type\": \"point\", // scatter points\n        \"opacity\": 0.3 // low opacity to appear subtle behind the line\n      },\n      \"encoding\": {\n        // LAYER-SPECIFIC ENCODING (supplementary)\n        \"y\": {\n          \"field\": \"temp_max\", // plot actual daily max temperature\n          \"title\": \"Max Temperature\" // legend/tooltip title for this mark\n        }\n      }\n    },\n    {\n      // LAYER 2: rolling mean (foreground line)\n      \"mark\": {\n        \"type\": \"line\", // continuous line mark\n        \"color\": \"red\", // line color (constant value, not data-driven)\n        \"size\": 3 // line thickness in pixels\n      },\n      \"encoding\": {\n        // LAYER-SPECIFIC ENCODING (supplementary)\n        \"y\": {\n          \"field\": \"rolling_mean\", // smoothed rolling mean values\n          \"title\": \"Rolling Mean of Max Temperature\" // tooltip/legend label\n        }\n      }\n    }\n  ]\n}'"
                }
