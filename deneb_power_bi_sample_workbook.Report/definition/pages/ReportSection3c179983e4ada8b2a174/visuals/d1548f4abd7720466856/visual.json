            "jsonSpec": {
              "expr": {
                "Literal": {
                  "Value": "'{\r\n  \"$schema\": \"https://vega.github.io/schema/vega-lite/v5.json\",\r\n  \"description\": \"A ranged dot plot showing change in life expectancy for major countries (1955â€“2000).\",\r\n\r\n  \"data\": {\"name\": \"dataset\"},\r\n\r\n  // TRANSFORMS (filter data before plotting)\r\n  \"transform\": [\r\n    {\r\n      // include only five most populous countries\r\n      \"filter\": {\r\n        \"field\": \"country\",\r\n        \"oneOf\": [\"China\", \"India\", \"United States\", \"Indonesia\", \"Brazil\"]\r\n      }\r\n    },\r\n    {\r\n      // include only the two comparison years\r\n      \"filter\": {\r\n        \"field\": \"year\",\r\n        \"oneOf\": [1955, 2000]\r\n      }\r\n    }\r\n  ],\r\n\r\n  // GLOBAL ENCODING (shared axes for all layers)\r\n  \"encoding\": {\r\n    \"x\": {\r\n      \"field\": \"life_expect\", // numeric measure of life expectancy\r\n      \"type\": \"quantitative\", // continuous variable\r\n      \"title\": \"Life Expectancy (years)\" // x-axis label\r\n    },\r\n    \"y\": {\r\n      \"field\": \"country\", // each country gets a separate horizontal line\r\n      \"type\": \"nominal\", // discrete categorical axis\r\n      \"title\": \"Country\",\r\n      \"axis\": {\r\n        \"offset\": 5, // slight gap between axis and marks\r\n        \"ticks\": false, // remove tick marks\r\n        \"minExtent\": 70, // allocate enough space for country labels\r\n        \"domain\": false // hide baseline line for a cleaner look\r\n      }\r\n    }\r\n  },\r\n\r\n  // LAYERED VISUALIZATION\r\n  \"layer\": [\r\n    {\r\n      // LAYER 1: connecting line between two years per country\r\n      \"mark\": \"line\", // draws straight line between paired points\r\n      \"encoding\": {\r\n        // DETAIL CHANNEL (layer-specific)\r\n        // groups data by country so each line connects only that country''s two points\r\n        \"detail\": {\r\n          \"field\": \"country\",\r\n          \"type\": \"nominal\"\r\n        },\r\n        // static color for connecting line\r\n        \"color\": {\"value\": \"#db646f\"} // soft red connector line\r\n      }\r\n    },\r\n    {\r\n      // LAYER 2: endpoint dots (for start and end year values)\r\n      \"mark\": {\r\n        \"type\": \"point\",\r\n        \"filled\": true // fill circles instead of open outlines\r\n      },\r\n      \"encoding\": {\r\n        // COLOR CHANNEL (distinguish years)\r\n        \"color\": {\r\n          \"field\": \"year\", // year determines circle color\r\n          \"type\": \"ordinal\", // ordered categories (1955 < 2000)\r\n          \"scale\": {\r\n            // define explicit order and matching colors\r\n            \"domain\": [1955, 2000], // ensures color order follows time\r\n            \"range\": [\"#e6959c\", \"#911a24\"] // lighter for 1955, darker for 2000\r\n          },\r\n          \"title\": \"Year\" // legend title\r\n        },\r\n        \"size\": {\"value\": 100}, // circle size in pixels (consistent across all points)\r\n        \"opacity\": {\"value\": 1} // fully opaque (points clearly visible)\r\n      }\r\n    }\r\n  ]\r\n}'"
